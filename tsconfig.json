{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "moduleResolution": "node",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "ct-mui": ["src"],
      "ct-mui/*": ["src/*", "*"]
    },
    "emitDeclarationOnly": false,
    "declarationDir": "./dist/types"
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
